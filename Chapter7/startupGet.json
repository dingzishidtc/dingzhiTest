[
  {
    "description": "模拟一个没有参数的get请求",
    "request": {
      "uri": "/getdemo",
      "method": "get"
    },
    "response": {
      "headers": {
      "Content-Type": "text/html;charset=gbk"
    },
      "text": "这是一个没有参数的get请求啊"
    }
  },
  {
    "description": "这是一个带参数的get请求",
    "request": {
      "uri": "/getwithparam",
      "method": "get",
      "queries": {
        "name": "dingzhi",
        "age": "18"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text": "我丁枝又回来了"
    }
  }
]