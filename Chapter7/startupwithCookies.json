[
  {
    "description": "这是一个返回cookies的get请求",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
  "response": {
    "cookies": {
      "login": "true"
    },
    "headers": {
      "Content-Type": "text/html;charset=gbk"
    },
    "text": "恭喜你获取cookies信息成功"
  }
  },
  {
    "description": "带cookies的get请求",
    "request": {
      "uri": "/get/with/cookies",
      "method": "get",
      "cookies": {
        "login": "true"
      }
    },
    "response": {
      "headers": {
        "Content-Type": "text/html;charset=gbk"
      },
      "text": "登录成功"
    }
  },


  {
    "description": "这是一个带参数的post请求",
    "request": {
      "uri": "/post/with/cookies",

      "method": "post",

      "headers": {"content-type": "application/json"},

      "cookies": {"login": "true"},

      "json": {
        "name": "dingzhi",
        "age": "18"
      }

    },
    "response": {
      "headers": {"Content-Type": "text/html;charset=gbk"},

      "status": 200,

      "json": {
        "huhansan": "success",
        "status": "1"
      }
    }
  }
]